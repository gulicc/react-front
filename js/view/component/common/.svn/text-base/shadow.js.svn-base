import React from "react";
import Style from "./css/shadow.css";

export default class Shadow extends React.Component {
    constructor(props){
        super(props)
    }

    render() {
        return(
            <div className={"flex flex-row flex-center align-center " + Style.shadow} id="shadow">
                {this.props.children}
            </div>
        )
    }
}