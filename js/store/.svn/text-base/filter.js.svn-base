let Filter = {
    //判断百分比数据 范围1-100
    checkPercent: (value) => {
        if((!isNaN(value) && value >= 1 && value <= 100) || value === "" || value === null) {
            return true;
        }else {
            return false;
        }
    },

    //判断数字
    checkNum: (value) => {
        if(!isNaN(value) || value === "" || value === null) {
            return true;
        }else {
            return false;
        }
    },
}

module.exports = Filter;